<Window x:Class="WiinUSoft.Windows.SyncWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WiinUSoft.Windows"
        mc:Ignorable="d"
        Title="Sync Window" Height="210" Width="500" Icon="../Images/WiinUSoft_Icon32.png" Background="{DynamicResource BrushSocialBlue}" Closing="Window_Closing" Loaded="Window_Loaded">
    <Grid>
        <ScrollViewer Name="scroll">
            <TextBox x:Name="prompt" TextWrapping="Wrap" Margin="10,10,10,10" IsReadOnly="False" Background="{DynamicResource BrushAntemWhite}"/>
        </ScrollViewer>
    </Grid>
</Window>
